#include <stdio.h>

int main() {
    int arr[5]={12,4,6,8,10};
    int max;
    max=arr[0];
    int min;
    min=arr[0];
    for(int i=1;i<5;i++)
    {
        if(max<arr[i])
        {
            max=arr[i];
            }
            else if(min>arr[i])
            {
                min=arr[i];
            }
            }
            printf("%d\n",max);
            printf("%d\n",min);
}
        
    
    

