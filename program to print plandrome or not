#include <stdio.h>

int main() 
 {
    int n=131;
    int rev=0;

    int r;
    int n1=n;
    while(n>0)
    {
        r=n%10;
        rev=rev*10+r;
        n=n/10;    
    }
    if(n1==rev)
    {
        printf(" it is a palindrome");
    }
    else
    {
        printf("it is not a palindromr");
    }
}
