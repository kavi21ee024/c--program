#include <stdio.h>

int main() 
{
    struct students
    {
        char name[20];
        int age ;
        float cgpa;
    };
    struct students s1={"kavi",19,8.15};
    struct students s2={"abi",19,9.8};
    printf("%s\n",s1.name);
    printf("%d\n",s1.age);
    printf("%f\n",s1.cgpa);
    printf("%s\n",s2.name);
    printf("%d\n",s2.age);
    printf("%f\n",s2.cgpa);
    return 0;
}
    
    
    
    
